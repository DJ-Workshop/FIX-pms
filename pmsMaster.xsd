<xs:schema targetNamespace="http://www.aginfo.jp/PMS/FIX-pms/XMLSchema"
    elementFormDefault="qualified"
    xmlns="http://www.aginfo.jp/PMS/FIX-pms/XMLSchema"
    xmlns:fp="http://www.aginfo.jp/PMS/FIX-pms/XMLSchema"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
  <xs:include schemaLocation="./pmsCommon.xsd"/>

  <!--**********-->
  <!--マスタ情報-->
  <!--**********-->
  
  <!-- 人物関係 -->
  <xs:complexType name="personType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">人物情報</xs:documentation>
      <xs:documentation xml:lang="en">Declares data type for personal / organization information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="PersonID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農家・作業者・組織などの識別コード（正整数）</xs:documentation>
          <xs:documentation xml:lang="en">Positive numeric identifier for farmer, worker, organization and so on</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="PersonCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農家・作業者・組織などの識別コード（文字列）</xs:documentation>
          <xs:documentation xml:lang="en">Identifier for farmer, worker, organization and so on</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Address">
        <xs:annotation>
          <xs:documentation xml:lang="ja">agroXML:NameAndAddressTypeを参考に定義したユーザ情報。人や組織・団体の名称・住所などのデータ型を定義しています。</xs:documentation>
          <xs:documentation xml:lang="en">Defines the name, address and other profiles of person, organization, and so on.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Contact" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Elements for values of contact informations like phone and mail.</xs:documentation>
          <xs:documentation xml:lang="ja">電話番号やメールアドレスなどの連絡先（コンタクト）情報を定義しています。</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Birth" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">誕生日・設立日など（日付型）</xs:documentation>
          <xs:documentation xml:lang="en">Birthday of person / established date of organization, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Age" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">年齢</xs:documentation>
          <xs:documentation xml:lang="en">Age of person or orgainization</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Note" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">任意の目的に使用するメモ（備考欄）100文字以内</xs:documentation>
          <xs:documentation xml:lang="en">Memorandum with at most 100 letters</xs:documentation>
        </xs:annotation>
      </xs:element>
		</xs:sequence>
  </xs:complexType>

  <xs:element name="Person" type="personType">
		<xs:annotation>
			<xs:documentation xml:lang="ja">個人・組織の基本情報要素です（→ personType型）</xs:documentation>
      <xs:documentation xml:lang="en">Contains personal / organizational information defined by &lt;personType&gt;</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="Member">
		<xs:annotation>
			<xs:documentation xml:lang="ja">メンバ（地主・農家，取引先（仕入れ先，顧客）などの個人・法人）情報要素です</xs:documentation>
      <xs:documentation xml:lang="en">Contains member / stakeholder information of farm such as owner of fields, suppliers of materials, customers of products and so on</xs:documentation>
		</xs:annotation>
    <xs:complexType>
      <xs:annotation>
        <xs:documentation xml:lang="ja">地主・農家・取引先情報：人物情報を拡張して，属性，地区などを追加</xs:documentation>
        <xs:documentation xml:lang="en">Defined as extension of &lt;personType&gt; with addition of some attributes</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
        <xs:extension base="personType">
          <xs:sequence>
            <xs:element ref="ResidentialArea" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">農家・組織等の住所地区名</xs:documentation>
                <xs:documentation xml:lang="en">Name of residential area of farmer, organization</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MemberFlag" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">組合員か否か</xs:documentation>
                <xs:documentation xml:lang="en">Element indicating whether member or not</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="CustomerFlag" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">顧客（出荷先）か否か</xs:documentation>
                <xs:documentation xml:lang="en">Element indicating whether customer or not</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="SupplierFlag" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">仕入れ先か否か</xs:documentation>
                <xs:documentation xml:lang="en">Element indicating whether supplier or not</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="JAN" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">項目に対するJAN相当コード文字列（最大20文字）：各種マスタ・台帳で使用される</xs:documentation>
                <xs:documentation xml:lang="en">JAN code with at most 20 letters</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MemberNote" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">農家・組織等についての特記事項</xs:documentation>
                <xs:documentation xml:lang="en">Memo about member</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

	<xs:element name="Worker">
		<xs:annotation>
			<xs:documentation xml:lang="ja">作業者情報要素です</xs:documentation>
      <xs:documentation xml:lang="en">Contains farm worker information</xs:documentation>
		</xs:annotation>
    <xs:complexType>
      <xs:annotation>
        <xs:documentation xml:lang="ja">作業者情報：人物情報（personType）を拡張して，給料単価を追加</xs:documentation>
        <xs:documentation xml:lang="en">Defined as extension of &lt;personType&gt; with addition of some attributes for worker</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
        <xs:extension base="personType">
          <xs:sequence>
            <xs:element ref="UnitCost" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">作業者の単価（月給，日給，時給など）</xs:documentation>
                <xs:documentation xml:lang="en">Unit pay of worker</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="JAN" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">項目に対するJAN相当コード文字列（最大20文字）：各種マスタ・台帳で使用される</xs:documentation>
                <xs:documentation xml:lang="en">JAN code with at most 20 letters</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="WorkerNote" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">作業者についての特記事項</xs:documentation>
                <xs:documentation xml:lang="en">Memo about worker</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="cropType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">作物名：本当なら作物リストから選択（制限）したい</xs:documentation>
      <xs:documentation xml:lang="en">Declares data type representing crop information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="CropID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">作物コード（正整数）：作物マスタで定義され，作付情報など各所で参照される</xs:documentation>
          <xs:documentation xml:lang="en">Positive numeric identifier of crop</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="CropCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">作物コード（文字列型）</xs:documentation>
          <xs:documentation xml:lang="en">String coded identifier of crop</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="CropName">
        <xs:annotation>
          <xs:documentation xml:lang="ja">作物名</xs:documentation>
          <xs:documentation xml:lang="en">Crop name</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- 作物関係 -->
  <xs:element name="CropList" type="cropType">
		<xs:annotation>
			<xs:documentation xml:lang="ja">作物情報要素です（→ cropType型）</xs:documentation>
      <xs:documentation xml:lang="en">Contains crop information</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="Crop">
		<xs:annotation>
			<xs:documentation xml:lang="ja">作物・品種情報要素です</xs:documentation>
      <xs:documentation xml:lang="en">Contains crop variety information</xs:documentation>
		</xs:annotation>
    <xs:complexType>
      <xs:annotation>
        <xs:documentation xml:lang="ja">品種名：本当なら品種リストから選択（制限）したい</xs:documentation>
        <xs:documentation xml:lang="en">Defined as extension of &lt;cropType&gt; with addition of elements representing variety information</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
        <xs:extension base="cropType">
          <xs:sequence>
            <xs:element ref="VarietyID" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">品種コード（正整数）：作物マスタで作物ごとに定義され（作物依存=作物ごとにユニーク），作付情報など各所で参照される</xs:documentation>
                <xs:documentation xml:lang="en">Positive numeric identifier of crop variety</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="VarietyCode" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">品種コード（文字列型）</xs:documentation>
                <xs:documentation xml:lang="en">String coded identifier of crop variety</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="VarietyName">
              <xs:annotation>
                <xs:documentation xml:lang="ja">品種名</xs:documentation>
                <xs:documentation xml:lang="en">Crop variety name</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="Yield" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">収量</xs:documentation>
                <xs:documentation xml:lang="en">Yield value</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="SeedAmount" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">反当種子量</xs:documentation>
                <xs:documentation xml:lang="en">Default amount of seed necessary to planting in unit area of field</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="SeedingFrom" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">播種開始日</xs:documentation>
                <xs:documentation xml:lang="en">Date of starting the term of cultivation, usually the date of seeding</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="SeedingUpto" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">播種終了日</xs:documentation>
                <xs:documentation xml:lang="en">Date of ending the term of cultivation, usually the date of harvesting</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MaterialKindID" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">資材種別コード（正整数）：資材の種類を識別するコード番号，肥料は"9999"，農薬は"9998"で固定（予約），その他の資材は任意（ユーザ指定）</xs:documentation>
                <xs:documentation xml:lang="en">Positive numeric identifier of material kind</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MaterialKindCode" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">資材種別コード（文字列）</xs:documentation>
                <xs:documentation xml:lang="en">String coded identifier of material kind</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MaterialKind" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">資材種別名称</xs:documentation>
                <xs:documentation xml:lang="en">Name of material kind</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MaterialID" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">資材コード（正整数）：資材（肥料・農薬含む）を識別するコード番号，種別コードとペアで資材を特定する（キー），資材コードは資材種別依存で資材種別ごとにユニーク</xs:documentation>
                <xs:documentation xml:lang="en">Positive numeric identifier of material</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MaterialCode" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">資材コード（文字列）</xs:documentation>
                <xs:documentation xml:lang="en">String coded identifier of material</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="MaterialName" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">資材名称</xs:documentation>
                <xs:documentation xml:lang="en">Name of material</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="JAN" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">項目に対するJAN相当コード文字列（最大20文字）：各種マスタ・台帳で使用される</xs:documentation>
                <xs:documentation xml:lang="en">JAN code with at most 20 letters</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="Note" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="ja">任意の目的に使用するメモ（備考欄）100文字以内</xs:documentation>
                <xs:documentation xml:lang="en">Memorandum with at most 100 letters</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

	<xs:element name="Work">
		<xs:annotation>
			<xs:documentation xml:lang="ja">作業情報要素です</xs:documentation>
      <xs:documentation xml:lang="en">Contains work information</xs:documentation>
		</xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="WorkID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業コード（正整数）：作物（CropID）ごとに定義される作業の整数値識別コード，作物ごとにユニーク</xs:documentation>
            <xs:documentation xml:lang="en">Positive numeric identifier of farm work which depends on crop</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業コード（文字列）：作物（CropID）ごとに定義される作業の文字列識別コード，作物ごとにユニーク</xs:documentation>
            <xs:documentation xml:lang="en">String coded identifier of farm work which depends on crop</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkName">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業名</xs:documentation>
            <xs:documentation xml:lang="en">Name of farm work</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="CropID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作物コード（正整数）：作物マスタで定義され，作付情報など各所で参照される</xs:documentation>
            <xs:documentation xml:lang="en">Positive numeric identifier of crop</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="CropCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作物コード（文字列型）</xs:documentation>
            <xs:documentation xml:lang="en">String coded identifier of crop</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="CropName" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作物名</xs:documentation>
            <xs:documentation xml:lang="en">Crop name</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="ProduceWorkFlag" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場でなくロットを対象とした作業か？（真の場合はロット作業）</xs:documentation>
            <xs:documentation xml:lang="en">Logical flag whether the farm work is for production process</xs:documentation>
          </xs:annotation>
        </xs:element>
        <!--xs:element name="SeedingWorkFlag" type="xs:boolean"/-->
        <xs:element ref="JAN" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">項目に対するJAN相当コード文字列（最大20文字）：各種マスタ・台帳で使用される</xs:documentation>
            <xs:documentation xml:lang="en">JAN code with at most 20 letters</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="Note" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">任意の目的に使用するメモ（備考欄）100文字以内</xs:documentation>
            <xs:documentation xml:lang="en">Memorandum with at most 100 letters</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- 作型関係 -->
  <xs:element name="CroppingWork">
    <xs:annotation>
      <xs:documentation xml:lang="ja">作型における個々の作業情報です</xs:documentation>
      <xs:documentation xml:lang="en">Contains cropping type information</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="WorkID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業コード（正整数）：作物（CropID）ごとに定義される作業の整数値識別コード，作物ごとにユニーク</xs:documentation>
            <xs:documentation xml:lang="en">Positive numeric identifier of farm work which depends on crop</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業コード（文字列）：作物（CropID）ごとに定義される作業の文字列識別コード，作物ごとにユニーク</xs:documentation>
            <xs:documentation xml:lang="en">String coded identifier of farm work which depends on crop</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkName" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業名</xs:documentation>
            <xs:documentation xml:lang="en">Name of farm work</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkTermStart" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業時期開始日</xs:documentation>
            <xs:documentation xml:lang="en">Date of term start of cropping type</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkTermEnd" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業時期終了日</xs:documentation>
            <xs:documentation xml:lang="en">Date of term end of work</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="Note" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">任意の目的に使用するメモ（備考欄）100文字以内</xs:documentation>
            <xs:documentation xml:lang="en">Memorandum with at most 100 letters</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CroppingWorks">
    <xs:annotation>
      <xs:documentation xml:lang="ja">作型における一連の作業情報リストです</xs:documentation>
      <xs:documentation xml:lang="en">Contains list of cropping work informations</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CroppingWork" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作型における個々の作業情報です</xs:documentation>
            <xs:documentation xml:lang="en">Contains cropping type information</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CroppingType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">作型情報要素です</xs:documentation>
      <xs:documentation xml:lang="en">Contains cropping type information</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CroppingTypeID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作型コード（正整数）</xs:documentation>
            <xs:documentation xml:lang="en">Positive numeric identifier of croping type(cultivation system)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="CroppingTypeCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作型コード（文字列型）</xs:documentation>
