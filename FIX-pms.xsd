          <xs:documentation xml:lang="en">Positive numeric identifier of field</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="FieldCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">圃場コード：圃場を識別するコード文字列（対人用）</xs:documentation>
          <xs:documentation xml:lang="en">String coded identifier of field</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="CroppingNo">
        <xs:annotation>
          <xs:documentation xml:lang="ja">対象圃場の作付番号（正整数）</xs:documentation>
          <xs:documentation xml:lang="en">Positive numeric identifier of croping No.</xs:documentation>
        </xs:annotation>
      </xs:element>
			<xs:element ref="CroppingName" minOccurs="0">
