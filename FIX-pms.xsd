<xs:schema targetNamespace="http://www.aginfo.jp/PMS/FIX-pms/XMLSchema"
    elementFormDefault="qualified"
    xmlns="http://www.aginfo.jp/PMS/FIX-pms/XMLSchema"
    xmlns:fp ="http://www.aginfo.jp/PMS/FIX-pms/XMLSchema"
    xmlns:gpx="http://www.topografix.com/GPX/1/1"
    xmlns:gml="http://www.opengis.net/gml"
		xmlns:bix="http://w3.fb.u-tokai.ac.jp/BIX-pp/XMLSchema"
    xmlns:bfd="http://naro.narc.fmd.go.jp/bfd"
		xmlns:xs ="http://www.w3.org/2001/XMLSchema"
>
	<xs:import namespace="http://www.opengis.net/gml" schemaLocation="./Gml.xsd"/>
  <xs:import namespace="http://www.topografix.com/GPX/1/1" schemaLocation="./gpx.xsd"/>
	<xs:import namespace="http://w3.fb.u-tokai.ac.jp/BIX-pp/XMLSchema" schemaLocation="./BIX-pp.xsd"/>
  <xs:import namespace="http://naro.narc.fmd.go.jp/bfd" schemaLocation="./bfd_MIndices.xsd"/>

  <xs:include schemaLocation="./pmsCommon.xsd"/>
  <xs:include schemaLocation="./pmsMaster.xsd"/>
  <xs:include schemaLocation="./pmsWorkProcess.xsd"/>
  <xs:include schemaLocation="./pmsProduct.xsd"/>
  <xs:include schemaLocation="./pmsProductOut.xsd"/>

  <!-- スキーマ情報 -->
  <xs:annotation>
    <xs:appinfo>FIX-pms schema version 1.1.2</xs:appinfo>
    <xs:documentation xml:lang="ja">農業生産工程管理データ交換標準書式 FIX-pms のスキーマを定義しています。</xs:documentation>
    <xs:documentation xml:lang="en">This .xsd document defines the xml schema of 'FIX-pms' as one of common data exchange formats for farm management data.</xs:documentation>
  </xs:annotation>

  <!--空間情報（GML引用）-->
	<xs:element name="SpatialData">
		<xs:annotation>
			<xs:documentation xml:lang="ja">関連する要素の空間情報を点，線，面の座標値として格納します</xs:documentation>
      <xs:documentation xml:lang="en">Store geospatial data in form of point, linestring, polygon</xs:documentation>
		</xs:annotation>
    <xs:complexType>
      <xs:annotation>
        <xs:documentation xml:lang="ja">OpenGISのGMLに従い，定義空間データ（GISデータ）型を定義しています</xs:documentation>
        <xs:documentation xml:lang="en">Define the spatial data type referring GML geospatial element</xs:documentation>
      </xs:annotation>
      <xs:sequence>
        <xs:element ref="gml:Point" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場要素では区画代表点（中心点）を保持します。</xs:documentation>
            <xs:documentation xml:lang="en">Store geospatial point data</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="gml:LineString" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="ja">作業要素では作業軌跡（地点の一次元集合）を保持します。通常作業軌跡表現にはGPXXが使用されますが，単純な軌跡情報としての利用も可能です。</xs:documentation>
            <xs:documentation xml:lang="en">Store geospatial linestring data</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="gml:Polygon" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場要素では区画形状（頂点集合）を保持します。</xs:documentation>
            <xs:documentation xml:lang="en">Store geospatial polygon data</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attributeGroup ref="acls">
        <xs:annotation>
          <xs:documentation xml:lang="ja">この要素のアクス件情報を保持します。</xs:documentation>
          <xs:documentation xml:lang="en">Store access control designator of this element</xs:documentation>
        </xs:annotation>
      </xs:attributeGroup>
    </xs:complexType>
  </xs:element>

	<!-- 計測情報（BIX-pp引用）：将来的には他の規格も並べて引用可 -->
	<xs:element name="MeasuredData">
		<xs:annotation>
			<xs:documentation xml:lang="ja">固定設置センサによる連続計測データを保持する要素です。現在はBIX-pp3.0によるデータのみです。</xs:documentation>
      <xs:documentation xml:lang="en">Store stational and time-series(domain) measured data</xs:documentation>
		</xs:annotation>
    <xs:complexType>
      <xs:annotation>
        <xs:documentation xml:lang="ja">固定設置センサ等による連続計測データを保持します。内部では東海大学で提唱されている"BIX-pp 3.0"規格を引用しています。将来的には他の規格もシーケンスに加えるという含み（拡張性）を残しています。</xs:documentation>
        <xs:documentation xml:lang="en">Define the data structure of stational and time domain measured data referring the BIX-pp 3.0 advocated by Dr.Hoshi(Kinki univ. in Jpn.)</xs:documentation>
      </xs:annotation>
      <xs:sequence>
        <xs:element ref="bix:BIX-pp" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="ja">BIX-pp規格による連続計測データ表現を定義しています。</xs:documentation>
            <xs:documentation xml:lang="en">Refer the BIX-pp element</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attributeGroup ref="acls">
        <xs:annotation>
          <xs:documentation xml:lang="ja">この要素のアクス件情報を保持します。</xs:documentation>
          <xs:documentation xml:lang="en">Store access control designator of this element</xs:documentation>
        </xs:annotation>
      </xs:attributeGroup>
    </xs:complexType>
  </xs:element>

	<!--農場情報（管理ファイル情報を含む）-->
  <!-- 型定義 -->
  <xs:complexType name="farmType">
    <xs:annotation>
      <xs:documentation xml:lang="jp">「農場」タイプを定義しています</xs:documentation>
      <xs:documentation xml:lang="en">Defines the data structure about farm information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FarmID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農場を識別するコード：現バージョンでは正整数です</xs:documentation>
          <xs:documentation xml:lang="en">A numeric code that identifies farm</xs:documentation>
        </xs:annotation>
      </xs:element>
			<xs:element ref="FarmCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ja">農場を識別するコード：上記の正整数に対して，こちらは文字列コードを想定</xs:documentation>
          <xs:documentation xml:lang="en">A literal code that identifies farm</xs:documentation>
        </xs:annotation>
			</xs:element>
      <xs:element ref="FarmName" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農場名称</xs:documentation>
          <xs:documentation xml:lang="en">Name of farm</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="FarmerID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農家ID：正整数値</xs:documentation>
          <xs:documentation xml:lang="en">A numeric code that identifies farmer ( farm owner )</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="FarmerCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農家コード：文字列</xs:documentation>
          <xs:documentation xml:lang="en">A literal code that identifies farmer ( farm owner )</xs:documentation>
        </xs:annotation>
      </xs:element>
			<xs:element ref="FarmerName" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ja">農家名</xs:documentation>
          <xs:documentation xml:lang="en">Name of farmer ( farm owner )</xs:documentation>
        </xs:annotation>
			</xs:element>
      <xs:element ref="Address" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農場の住所等</xs:documentation>
          <xs:documentation xml:lang="en">Address information of farm</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Contact" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農場の連絡先情報</xs:documentation>
          <xs:documentation xml:lang="en">Contact information of farm</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="FileID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">このドキュメントインスタンスに対応するPMSの管理ファイル識別コード</xs:documentation>
          <xs:documentation xml:lang="ja">Numeric ID related to this XML document instance defined in PMS</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="FileName" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">前項管理ファイルコードに対応する管理ファイルの識別名称</xs:documentation>
          <xs:documentation xml:lang="en">File name of farm management data set</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Note" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農場についての備考・任意のメモ</xs:documentation>
          <xs:documentation xml:lang="en">Any notes about farm</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="FarmGuid" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">農場を識別するGUIDコード</xs:documentation>
          <xs:documentation xml:lang="en">A literal guid that identifies farm</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="FileGuid" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">対応する管理ファイルを識別するGUIDコード</xs:documentation>
          <xs:documentation xml:lang="en">A literal guid that identifies farm management data set</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="acls">
      <xs:annotation>
        <xs:documentation xml:lang="ja">この要素のアクス件情報を保持します。</xs:documentation>
        <xs:documentation xml:lang="en">Store access control designator of this element</xs:documentation>
      </xs:annotation>
    </xs:attributeGroup>
  </xs:complexType>
  <!--農場要素-->
	<xs:element name="Farm" type="farmType">
		<xs:annotation>
			<xs:documentation xml:lang="ja">農場情報要素です（→ farmType型）</xs:documentation>
      <xs:documentation xml:lang="en">Store farm infomation in the form of "farmType" type</xs:documentation>
		</xs:annotation>
  </xs:element>

  <!--台帳情報-->
  
  <!--土地台帳：単一圃場情報型-->
	<!-- 部分圃場要素 -->
	<xs:element name="PartField">
		<xs:annotation>
			<xs:documentation xml:lang="ja">「部分圃場」要素です。通常，耕作圃場内の分筆区画や合筆圃場内の地主別区画を保持します。また，圃場内を細分化して管理したい場合にも使用します。</xs:documentation>
      <xs:documentation xml:lang="en">Stores information about partial block of field</xs:documentation>
		</xs:annotation>
    <!-- 部分圃場型 -->
    <xs:complexType>
      <xs:annotation>
        <xs:documentation xml:lang="ja">部分圃場タイプを定義しています on 25-May-2012 by D.J.</xs:documentation>
        <xs:documentation xml:lang="en">Defines data structure of partial field information</xs:documentation>
      </xs:annotation>
      <xs:sequence>
        <xs:element ref="FieldID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場連番値（シリアル番号）</xs:documentation>
            <xs:documentation xml:lang="en">Serial number assigned to each field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="FieldCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場コード</xs:documentation>
            <xs:documentation xml:lang="en">Literal code assigned to each field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ParentFieldID" type="positiveCodeType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">この「部分圃場（区画）」を保持する親圃場ID（連番値）を保持します</xs:documentation>
            <xs:documentation xml:lang="en">Stores the ID of parent field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="FieldNumber" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">地番</xs:documentation>
            <xs:documentation xml:lang="en">Lot number of each field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="FieldName" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">地名（圃場名）</xs:documentation>
            <xs:documentation xml:lang="en">Name of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="RegisteredArea" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">地籍（登記）面積</xs:documentation>
            <xs:documentation xml:lang="en">Cadastral acreage</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="CultivateArea" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">耕作面積・水張り面積</xs:documentation>
            <xs:documentation xml:lang="en">Tillage acreage</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="OwnerID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">地主（圃場所有者）ID：農家・組織マスタの PersonID を引用</xs:documentation>
            <xs:documentation xml:lang="en">Numerical Identifier assigned to the owner of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="OwnerCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">地主（圃場所有者）コード：農家・組織マスタの PersonalCode を引用</xs:documentation>
            <xs:documentation xml:lang="en">Literal code assigned to the owner of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="OwnerName" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">地主名</xs:documentation>
            <xs:documentation xml:lang="en">Owner name of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkerID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">耕作担当者ID：農家・組織マスタの PersonID を引用</xs:documentation>
            <xs:documentation xml:lang="en">Numerical identifier assigned to the tiller of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkerCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">耕作担当者コード</xs:documentation>
            <xs:documentation xml:lang="en">Literal code assigned to the tiller of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="WorkerName" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">耕作担当者名</xs:documentation>
            <xs:documentation xml:lang="en">Tiller name of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="MowerID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">畦畔管理担当者ID：農家・組織マスタの PersonID を引用</xs:documentation>
            <xs:documentation xml:lang="en">Numerical identifier assigned to the mowing worker of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="MowerCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">畦畔管理担当者コード</xs:documentation>
            <xs:documentation xml:lang="en">Literal code assigned to the mowing worker of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="MowerName" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">畦畔管理者名</xs:documentation>
            <xs:documentation xml:lang="en">Name of the mowing worker of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="IrrigatorID" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">水管理担当者ID：農家・組織マスタの PersonID を引用</xs:documentation>
            <xs:documentation xml:lang="en">Numerical identifier assigned to the irrgating worker of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="IrrigatorCode" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">水管理担当者コード</xs:documentation>
            <xs:documentation xml:lang="en">Literal code assigned to the irrgating worker of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="IrrigatorName" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">水管理担当者名</xs:documentation>
            <xs:documentation xml:lang="en">Name of the irrigating worker of field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="JAN" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場を識別するJAN相当コード（最大20文字）</xs:documentation>
            <xs:documentation xml:lang="en">JAN code assigned to each field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="Note" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">備考・任意のメモ</xs:documentation>
            <xs:documentation xml:lang="en">Note, comment and so on</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="SpatialData" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場区画の空間データ</xs:documentation>
            <xs:documentation xml:lang="en">Spatial data of each field</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="PartFields" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">圃場内の部分区画</xs:documentation>
            <xs:documentation xml:lang="en">Partial fields in each field if any</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attributeGroup ref="acls">
        <xs:annotation>
          <xs:documentation xml:lang="ja">この要素のアクス件情報を保持します。</xs:documentation>
          <xs:documentation xml:lang="en">Store access control designator of this element</xs:documentation>
        </xs:annotation>
      </xs:attributeGroup>
    </xs:complexType>
  </xs:element>
	<!-- 部分圃場コンテナ要素 -->
	<xs:element name="PartFields">
		<xs:annotation>
			<xs:documentation xml:lang="ja">「部分圃場」コンテナです。圃場内の複数の部分圃場を保持します。</xs:documentation>
      <xs:documentation xml:lang="en">Stores information of partial fields</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
        <xs:element ref="PartField" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="ja">部分圃場情報</xs:documentation>
            <xs:documentation xml:lang="en">Part field information</xs:documentation>
          </xs:annotation>
        </xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- 単一圃場型 -->
  <xs:complexType name="fieldType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">「圃場」タイプを定義しています</xs:documentation>
      <xs:documentation xml:lang="en">Defines "field" information data type</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FieldID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="ja">圃場連番値（シリアル番号）</xs:documentation>
          <xs:documentation xml:lang="en">Serial number assigned to each field</xs:documentation>
        </xs:annotation>
      </xs:element>
