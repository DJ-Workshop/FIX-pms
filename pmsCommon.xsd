          <xs:annotation>
            <xs:documentation xml:lang="ja">カルシウム成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Calcium in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Mg" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">マグネシウム成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Magnesium content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="S" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">硫黄成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Sulfur content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Mn" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">マンガン成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Manganese content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="B" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">ホウ素成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Boron content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Fe" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">鉄成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Iron content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Cu" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">銅成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Copper content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Zn" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">亜鉛成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Zinc content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Cl" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">塩素成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Chlorine content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Mo" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">モリブデン成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Molybdenum content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Si" type="ingredientWeightPercentType" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">ケイ素成分含量（重量百分率）</xs:documentation>
            <xs:documentation xml:lang="en">Silicon content in weight percent</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="RegisteredChemical">
    <xs:annotation>
      <xs:documentation xml:lang="ja">資材を拡張する農薬独自のマスタ情報：農薬登録情報など</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RegNo" type="xs:positiveInteger">
          <xs:annotation>
            <xs:documentation xml:lang="ja">農薬登録番号（農薬取締法に基づく）</xs:documentation>
            <xs:documentation xml:lang="en">Registered number of agricultural chemical according to the domestic reguration law</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="RegName" type="xs:normalizedString" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">農薬登録名</xs:documentation>
            <xs:documentation xml:lang="en">Register name of agricultural chemical</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Kind" type="xs:normalizedString" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">種別</xs:documentation>
            <xs:documentation xml:lang="en">Kind of agric. chemical</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Maker" type="xs:normalizedString" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">メーカ名</xs:documentation>
            <xs:documentation xml:lang="en">Manufacturer of agric. chemical</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Ingredient" type="xs:normalizedString" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">農薬成分</xs:documentation>
            <xs:documentation xml:lang="en">Ingredient of agric. chemical</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Usage" type="xs:normalizedString" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">用途</xs:documentation>
            <xs:documentation xml:lang="en">Usage of agric. chemical</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Type" type="xs:normalizedString" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">剤形</xs:documentation>
            <xs:documentation xml:lang="en">Type of agric. chemical</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Application" type="xs:normalizedString" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ja">適用</xs:documentation>
            <xs:documentation xml:lang="en">Application of agric. chemical</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!--生育情報関係-->
  <xs:element name="ItemCode" type="xs:nonNegativeInteger">
    <xs:annotation>
      <xs:documentation xml:lang="ja">ユーザ定義型データの項目ID値（非負値：0以上の整数）</xs:documentation>
      <xs:documentation xml:lang="en">Non-negative numeric identifier of user defined data item</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!-- added on 17-Nov-2011 by D.J. -->
  <xs:element name="ItemName" type="nameType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">ユーザ定義型データ項目名</xs:documentation>
      <xs:documentation xml:lang="en">Data item name of user defined data</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="NumericValue" type="numericValueWithUnitType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">数値データ</xs:documentation>
      <xs:documentation xml:lang="en">Data item with numeric value</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DataCode" type="xs:nonNegativeInteger">
    <xs:annotation>
      <xs:documentation xml:lang="ja">データコード</xs:documentation>
      <xs:documentation xml:lang="en">Data code of user defined data</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!-- added on 17-Nov-2011 by D.J. -->
  <xs:element name="DataName" type="nameType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">データ識別名・データ名称</xs:documentation>
      <xs:documentation xml:lang="en">Name of data item</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DataType" type="dataTypeStringType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">データ型記述子（1文字）</xs:documentation>
      <xs:documentation xml:lang="en">Data type identifier</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="UnitString" type="unitStringType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">単位文字列</xs:documentation>
      <xs:documentation xml:lang="en">Unit string of the data item</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ValueString" type="xs:normalizedString">
    <xs:annotation>
      <xs:documentation xml:lang="ja">文字列表現された数値データ</xs:documentation>
      <xs:documentation xml:lang="en">String coded value of user defined data item</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="BinaryString" type="binaryStringType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">バイナリデータ</xs:documentation>
      <xs:documentation xml:lang="en">Binary data of user defined data item</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="RecDate" type="xs:dateTime">
    <xs:annotation>
      <xs:documentation xml:lang="ja">データ記録日（計測日，生成日，登録日など）</xs:documentation>
      <xs:documentation xml:lang="en">Date on data recorded</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!-- changed 'date' to 'dateTime' on 04-Jul-2012 by D.J. -->
  <xs:element name="ProductYear" type="yearType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産年（西暦年）</xs:documentation>
      <xs:documentation xml:lang="en">Year on product was produced</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProductArea" type="areaType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産面積</xs:documentation>
      <xs:documentation xml:lang="en">Area of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProductYield" type="yieldType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産量（収量）</xs:documentation>
      <xs:documentation xml:lang="en">Gross yield of product</xs:documentation>
    </xs:annotation>
  </xs:element>

  <!--出荷荷姿情報の子要素-->
  <!--xs:element name="FormID" type="xs:positiveInteger"/-->
  <xs:element name="FormID" type="positiveCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">荷姿情報識別コード（整数値）</xs:documentation>
      <xs:documentation xml:lang="en">Positive numeric identifier of packaging style of materials and products</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="FormCode" type="stringCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">荷姿情報識別コード（文字列）</xs:documentation>
      <xs:documentation xml:lang="en">String coded identifier of packaging style of materials and products</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="FormName" type="nameType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">荷姿識別名</xs:documentation>
      <xs:documentation xml:lang="en">Name of packaging style</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Capacity" type="amountType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">荷姿容量</xs:documentation>
      <xs:documentation xml:lang="en">Capasity of package</xs:documentation>
    </xs:annotation>
  </xs:element>

  <!--生産出荷関係の子要素-->
  <!--xs:element name="ProductID" type="xs:positiveInteger"/-->
  <xs:element name="ProductID" type="positiveCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品識別コード（整数値）</xs:documentation>
      <xs:documentation xml:lang="en">Positive numeric identifier of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProductCode" type="stringCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品識別コード（文字列）</xs:documentation>
      <xs:documentation xml:lang="en">String coded identifier of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProductName" type="nameType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品名</xs:documentation>
      <xs:documentation xml:lang="en">Product name</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="FromField" type="xs:positiveInteger">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品の由来圃場番号</xs:documentation>
      <xs:documentation xml:lang="en">Positive numeric identifier of source field of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="FromLot" type="xs:positiveInteger">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品の由来ロット番号</xs:documentation>
      <xs:documentation xml:lang="en">Positive numeric identifier of source lot of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProductDate" type="xs:date">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産日</xs:documentation>
      <xs:documentation xml:lang="en">Date on the product is produced</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="UnitPrice" type="priceType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品の単価</xs:documentation>
      <xs:documentation xml:lang="en">Unit price of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProductCount" type="xs:nonNegativeInteger">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品の生産数</xs:documentation>
      <xs:documentation xml:lang="en">Number of product produced</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="TotalPrice" type="priceType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品総額：単価×生産数</xs:documentation>
      <xs:documentation xml:lang="en">Gross price of product : unit price * number of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ShipDate" type="xs:date">
    <xs:annotation>
      <xs:documentation xml:lang="ja">生産品の出荷日</xs:documentation>
      <xs:documentation xml:lang="en">Shipping date of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--xs:element name="CustomerID" type="xs:positiveInteger"/-->
  <xs:element name="CustomerID" type="positiveCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">顧客識別コード（整数値）</xs:documentation>
      <xs:documentation xml:lang="en">Positive numeric identifier of customer of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CustomerCode" type="stringCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">顧客識別コード（文字列）</xs:documentation>
      <xs:documentation xml:lang="en">String coded identifier of customer of product</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CustomerName" type="nameType">
    <xs:annotation>
      <xs:documentation xml:lang="ja">顧客名</xs:documentation>
      <xs:documentation xml:lang="en">Customer name</xs:documentation>
    </xs:annotation>
  </xs:element>

</xs:schema>
